{
  "name": "moti",
  "version": "0.0.6",
  "private": false,
  "keywords": [
    "react-native",
    "ios",
    "android",
    "web"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nandorojo/moti.git",
    "directory": "packages/moti"
  },
