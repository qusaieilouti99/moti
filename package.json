    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-native": "^0.63.30",
    "expo": "^40.0.0-beta.5",
    "react": "~16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "^0.17.0",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-reanimated": "2.0.0-rc.0",
    "react-native-web": "~0.14.9",
    "typescript": "^4.0.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
